{
  "name": "next-gen-ui-tests",
  "version": "1.0.0",
  "description": "next-gen ui tests",
  "scripts": {
    "cy:open:prod": "cypress open --config-file ./cypress/config/prod.config.js",
    "cy:open": "cypress open --config-file ./cypress/config/prod.config.js",
    "performance": "cypress run --record --parallel --tag prod-performance --group prod-performance --config-file ./cypress/config/prod.config.js --spec **/*performance.cy.js",
    "acceptance": "cypress run --record --parallel --tag prod-acceptance --group prod-acceptance --config-file ./cypress/config/prod.config.js --spec **/*acceptance.cy.js",
    "prod:acceptance": "cypress run --tag prod-acceptance --group prod-acceptance --parallel --ci-build-id ${{github.run_number}} --config-file ./cypress/config/prod.config.js --spec 'cypress/e2e/sftp/**/*'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cy-verify-downloads": "^0.2.0",
    "cypress": "^13.1.0",
    "cypress-downloadfile": "^1.2.3",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-network-idle": "^1.14.2",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^5.17.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-localstorage-commands": "2.2.2",
    "cypress-slow-down": "^1.2.1",
    "cypress-wait-until": "1.7.2",
    "dayjs": "^1.11.7",
    "puppeteer": "^19.8.0",
    "ssh2-sftp-client": "^9.1.0"
  }
}
